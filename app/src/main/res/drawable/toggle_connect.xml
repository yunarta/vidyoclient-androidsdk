<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <!-- When not selected, use callStart -->
    <item android:drawable="@drawable/callstart" android:state_checked="false" android:state_enabled="true" />
    <!-- When selected, use callEnd-->
    <item android:drawable="@drawable/callend" android:state_checked="true" android:state_enabled="true" />
    <item android:drawable="@drawable/calldisabled" android:state_enabled="false" />

</selector>